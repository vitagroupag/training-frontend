                            <items id="text">Right</items>
                            <items id="description">Right side of the body.</items>
                        </term_definitions>
                        <term_definitions code="at0008">
                            <items id="text">Circumference</items>
                            <items id="description">Measured girth of the identified body segment.</items>
                        </term_definitions>
                        <term_definitions code="at0009">
                            <items id="text">Comment</items>
                            <items id="description">Additional narrative about the circumference measurement of the body segment, not captured in other fields.</items>
                        </term_definitions>
                        <term_definitions code="at0010">
                            <items id="text">ItemTree</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0011">
                            <items id="text">Confounding factors</items>
                            <items id="description">Record any issues or factors that may impact on the measurement of the body segment.</items>
                            <items id="comment">For example: an uncooperative child.</items>
                        </term_definitions>
                        <term_definitions code="at0012">
                            <items id="text">Average measurement</items>
                            <items id="description">The average of several measurements of the same body segment taken during a single examination.</items>
                        </term_definitions>
